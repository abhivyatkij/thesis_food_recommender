  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
  <hasCookTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasCookTime>
  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">8 hours</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">International</hasCuisine>
