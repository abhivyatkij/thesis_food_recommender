  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P04-0004757</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.masalakorb.com/instant-pot-black-eyed-peas-curry-lobia-masala/</hasRecipeURL>
  <hasPrepTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">5 minutes</hasPrepTime>
  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
