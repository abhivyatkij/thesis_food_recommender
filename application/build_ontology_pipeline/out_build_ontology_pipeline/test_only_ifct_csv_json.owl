<owl:NamedIndividual rdf:about="#Tricolour Idli Recipe with Tricolour Chutneys  Vegetable Idli | Mini Idli Recipe (Button Idli) | Tiranga Idli  Republic Day Special">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#idli batter"/>
  <hasIngredient rdf:resource="#carrot"/>
  <hasIngredient rdf:resource="#green capsicum"/>
  <hasIngredient rdf:resource="#herbs coriander &amp; mint"/>
  <hasIngredient rdf:resource="#oil to grease idli plate"/>
  <hasIngredient rdf:resource="#salt"/>
