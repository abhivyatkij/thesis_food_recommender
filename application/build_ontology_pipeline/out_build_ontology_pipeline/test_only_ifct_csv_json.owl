<owl:NamedIndividual rdf:about="#Turmeric Ginger Tea Recipe | How to make Turmeric Tea With Fresh Turmeric Root | Immunity Tea">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#water"/>
