  <hasIngredient rdf:resource="#yellow cucumber"/>
  <hasIngredient rdf:resource="#oil or ghee"/>
  <hasIngredient rdf:resource="#garlic pods"/>
  <hasIngredient rdf:resource="#dry red chillies"/>
