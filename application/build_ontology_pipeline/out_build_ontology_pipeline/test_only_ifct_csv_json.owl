  <hasIngredient rdf:resource="#cardamom pods elaichi"/>
  <hasIngredient rdf:resource="#mace"/>
  <hasIngredient rdf:resource="#bay leaves"/>
  <hasIngredient rdf:resource="#kapok buds marathi moggu"/>
  <hasIngredient rdf:resource="#kalpasi black stone flower"/>
  <hasIngredient rdf:resource="#onion"/>
  <hasIngredient rdf:resource="#green chillies"/>
