<owl:NamedIndividual rdf:about="#Pesarattu Preparation | Instant Moong Dal Dosa Recipe | Andhra Pesarattu Dosa Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#whole green moong dal"/>
  <hasIngredient rdf:resource="#raw rice"/>
  <hasIngredient rdf:resource="#urad dal"/>
