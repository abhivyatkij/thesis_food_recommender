<owl:NamedIndividual rdf:about="#Coconut Burfi">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#grated coconut"/>
  <hasIngredient rdf:resource="#sugar"/>
  <hasIngredient rdf:resource="#water"/>
