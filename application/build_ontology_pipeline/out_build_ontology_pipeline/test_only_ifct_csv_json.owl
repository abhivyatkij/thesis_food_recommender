<owl:NamedIndividual rdf:about="#Crispy Minty Potato Samosas">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#allpurposeflour"/>
  <hasIngredient rdf:resource="#ajwaincaromseeds"/>
  <hasIngredient rdf:resource="#salt"/>
