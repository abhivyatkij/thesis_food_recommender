<owl:NamedIndividual rdf:about="#Easy Aviyal Recipe  How to make Avial Curry | Onam Sadya Aviyal Recipe | Vegetable Avial Recipe South Indian style">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#drumstick"/>
