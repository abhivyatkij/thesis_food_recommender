<owl:NamedIndividual rdf:about="#Shrimp and Pineapple Curry">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#light cooking oil"/>
  <hasIngredient rdf:resource="#finely minced garlic"/>
  <hasIngredient rdf:resource="#small onion"/>
  <hasIngredient rdf:resource="#thai red curry paste"/>
  <hasIngredient rdf:resource="#medium-sized shrimp"/>
  <hasIngredient rdf:resource="#coconut milk"/>
  <hasIngredient rdf:resource="#chopped pineapple"/>
  <hasIngredient rdf:resource="#salt"/>
  <hasIngredient rdf:resource="#fresh lime"/>
  <hasIngredient rdf:resource="#fresh cilantro leaves"/>
