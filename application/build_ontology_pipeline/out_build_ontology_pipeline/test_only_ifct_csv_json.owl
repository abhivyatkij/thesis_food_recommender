  <hasIngredient rdf:resource="#cumin seeds"/>
  <hasIngredient rdf:resource="#ginger"/>
  <hasIngredient rdf:resource="#onions"/>
  <hasIngredient rdf:resource="#red chilli powder"/>
  <hasIngredient rdf:resource="#turmeric powder"/>
  <hasIngredient rdf:resource="#tomatoes"/>
  <hasIngredient rdf:resource="#salt"/>
  <hasIngredient rdf:resource="#garam masala powder"/>
  <hasIngredient rdf:resource="#fresh cilantro"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['HOK']-P09-0013463</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.hookedonheat.com/turning-up-the-heat-baingan-bharta/</hasRecipeURL>
  <hasPrepTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">25 minutes</hasPrepTime>
  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
  <hasCookTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">15 minutes</hasCookTime>
  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">40 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
  <hasDiet rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDiet>
  <hasServings rdf:datatype="http://www.w3.org/2001/XMLSchema#string">4</hasServings>
  <hasInstructions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[{'heading': 'Instructions', 'instructions': 'Make tiny slits all over the eggplants and roast them in a 400 degree oven for 20-25 mins till the skin starts to char on all sides. Peel the eggplants, mash it to a pulp and set aside. Heat oil in a large pan and add in cumin seeds. As soon as they begin to sizzle, add in chopped ginger and onions, and fry for 2-3 minutes on medium-high heat till onions start to soften and lightly brown. Add in spices and chopped tomatoes, and fry for a few minutes till tomatoes start to pulp and give out oil along the sides. Season with salt, and mix in the mashed eggplant. Stir to mix well and cook covered on low heat for 3-4 minutes. Sprinkle garam masala on top and garnish with fresh cilantro leaves. Serve hot with a side of rotis.'}]</hasInstructions>
