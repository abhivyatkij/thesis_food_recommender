<owl:NamedIndividual rdf:about="#Easy Instant Pot Vegetable Biryani  Spicy Veg Biryani Recipe | Easy Vegetable Biryani Recipe | Veg Biryani Instant Pot Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#oil or ghee"/>
  <hasIngredient rdf:resource="#cardamom pod"/>
  <hasIngredient rdf:resource="#cinnamon stick"/>
