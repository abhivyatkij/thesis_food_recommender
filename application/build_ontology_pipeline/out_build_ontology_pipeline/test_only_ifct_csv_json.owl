<owl:NamedIndividual rdf:about="#Kesar Badam Kulfi | Almond Kulfi Recipe | How to make Kulfi at Home | Kulfi Ice Recipe | Kulfi Indian Dessert">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#full fat milk"/>
