  <hasIngredient rdf:resource="#mini frozen tart shells, unsweetened"/>
  <hasIngredient rdf:resource="#shredded cheese"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['HOK']-P06-0013365</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.hookedonheat.com/spinach-mushroom-feta-quiche/</hasRecipeURL>
