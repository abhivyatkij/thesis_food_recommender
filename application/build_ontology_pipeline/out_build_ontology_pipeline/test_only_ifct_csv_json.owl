<owl:NamedIndividual rdf:about="#Tandoori Chicken Wings  Oven and Air Fryer Version">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['HOK']-P06-0013364</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.hookedonheat.com/tandoori-chicken-wings-oven-and-air-fryer-version/</hasRecipeURL>
  <hasPrepTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10 minutes</hasPrepTime>
  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
  <hasCookTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">20 minutes</hasCookTime>
  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">26 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
  <hasDiet rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDiet>
  <hasServings rdf:datatype="http://www.w3.org/2001/XMLSchema#string">4</hasServings>
  <hasInstructions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[{'heading': 'Instructions', 'instructions': 'Marinate the chicken wings with the oil, tomato paste, spices, salt and fresh lemon juice. If you have the time, set aside to let it rest for about an hour.'}]</hasInstructions>
  <hasInstructions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[{'heading': 'For the Oven:', 'instructions': 'Preheat the oven at 425 degrees and roast the marinated chicken wings on a lined baking sheet for 15-20 minutes until the chicken is completely cooked through. Make sure to flip them over once in between to evenly brown on both sides. Garnish with fresh cilantro and serve hot with some Green Chutney on the side.'}]</hasInstructions>
  <hasInstructions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[{'heading': 'For the Air Fryer:', 'instructions': 'Bake them in an Air Fryer at 400 degrees for 12-15 minutes, turning them over once in between until the chicken is completely cooked through. Serve hot with some Green Chutney on the side.'}]</hasInstructions>
  <hasIngredientDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[{'heading': 'Ingredients', 'items': ['20-14 chicken wings', '2 tbsp light cooking oil', '1 tbsp tomato paste', '1 tbsp coriander powder', '1 tbsp tandoori masala', '1/4 tsp red chilli powder', '1/4 tsp garam masala', '1/2 tsp amchoor powder', 'salt to taste', '2 tbsp fresh lemon juice', 'a handful of fresh cilantro finely chopped for garnish']}]</hasIngredientDescription>
</owl:NamedIndividual>

