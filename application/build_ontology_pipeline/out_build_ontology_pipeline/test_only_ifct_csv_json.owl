<owl:NamedIndividual rdf:about="#Zucchini Tomato Chutney South Indian  Zucchini Pachadi | Zucchini Chutney Recipe Indian | Zucchini Tomato Pachadi Andhra Style">
  <rdf:type rdf:resource="#FoodRecipes"/>
