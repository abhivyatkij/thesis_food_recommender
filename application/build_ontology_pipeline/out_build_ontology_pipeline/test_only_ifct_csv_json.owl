<owl:NamedIndividual rdf:about="#Palak Gosht (Lamb &amp; Spinach Curry) in an Instant Pot">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#light cooking oil"/>
