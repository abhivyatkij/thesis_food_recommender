<owl:NamedIndividual rdf:about="#Gujarati Pad Wali Roti (Be Padi Rotli) | Two Layer Roti | Layered Chapati Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#whole_wheat_flour"/>
  <hasIngredient rdf:resource="#oil"/>
  <hasIngredient rdf:resource="#salt"/>
