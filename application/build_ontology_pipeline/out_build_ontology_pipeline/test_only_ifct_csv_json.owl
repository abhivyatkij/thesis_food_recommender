<owl:NamedIndividual rdf:about="#Allam Garelu  Ginger Vada | Minapa Garelu Recipe | Ginger Medu Vada Recipe | Urad Dal Vada Recipe | How to make South Indian Vada">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#urad dal"/>
  <hasIngredient rdf:resource="#green chilies"/>
  <hasIngredient rdf:resource="#ginger allam piece"/>
  <hasIngredient rdf:resource="#cumin seeds"/>
  <hasIngredient rdf:resource="#salt"/>
  <hasIngredient rdf:resource="#oil"/>
