<owl:NamedIndividual rdf:about="#Mango Kesari or Mango Sheera">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#semolina sooji/rava"/>
  <hasIngredient rdf:resource="#water"/>
  <hasIngredient rdf:resource="#sugar"/>
