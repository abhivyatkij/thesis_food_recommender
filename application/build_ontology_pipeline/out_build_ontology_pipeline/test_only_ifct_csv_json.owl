<owl:NamedIndividual rdf:about="#Yellow Cucumber Dal Or Dosakaya Pappu">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#pigeon peas split lentils tuvar dal/toor dal/kandhi pappu"/>
