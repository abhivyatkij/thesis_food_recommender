ategory': 'spices', 'form': 'NA', 'quantity': 1.0, 'unit': 'teaspoon', 'estimated_weight_in_grams': 2.0, 'notes': 'NA'}, 'cumin powder': {'category': 'spices', 'form': 'NA', 'quantity': 1.0, 'unit': 'teaspoon', 'estimated_weight_in_grams': 2.0, 'notes': 'NA'}, 'coriander powder': {'category': 'spices', 'form': 'NA', 'quantity': 1.0, 'unit': 'teaspoon', 'estimated_weight_in_grams': 2.0, 'notes': 'NA'}, 'fresh coriander': {'category': 'herbs and spices', 'form': 'finely chopped', 'quantity': 0.5, 'unit': 'cup', 'estimated_weight_in_grams': 8.0, 'notes': 'NA'}, 'fresh mint leaves': {'category': 'herbs and spices', 'form': 'finely chopped', 'quantity': 0.5, 'unit': 'cup', 'estimated_weight_in_grams': 8.0, 'notes': 'NA'}, 'onion': {'category': 'vegetables', 'form': 'NA', 'quantity': 1.0, 'unit': 'count', 'estimated_weight_in_grams': 100.0, 'notes': 'NA'}, 'lemon': {'category': 'fruit', 'form': 'cut into wedges', 'quantity': 1.5, 'unit': 'count', 'estimated_weight_in_grams': 90.0, 'notes': '1 to 2, wedges'}, 'carrots': {'category': 'root and tuberous vegetables', 'form': 'peeled, thinly sliced', 'quantity': 0.5, 'unit': 'cup', 'estimated_weight_in_grams': 65.0, 'notes': 'NA'}, 'chaat masala': {'category': 'spices', 'form': 'optional', 'quantity': 0.5, 'unit': 'teaspoon', 'estimated_weight_in_grams': 1.0, 'notes': 'NA'}}}]</hasIngredientDescription>
