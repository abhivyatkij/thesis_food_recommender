<owl:NamedIndividual rdf:about="#Frozen Watermelon Slush Recipe | Watermelon Lime Slush | How To Make Easy Slushies | Melon Slush Recipes | Summer Drink Recipes">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#watermelon pieces deseeded &amp; cut into 1 inch cubes"/>
  <hasIngredient rdf:resource="#lime or lemon juice"/>
