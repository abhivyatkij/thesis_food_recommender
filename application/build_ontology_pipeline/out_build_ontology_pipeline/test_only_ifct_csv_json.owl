<owl:NamedIndividual rdf:about="#Carrot Cake Sandwich Cookies Recipe With Cream Cheese Frosting | Celebrating 150th Blog Post">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#cream cheese"/>
  <hasIngredient rdf:resource="#unsalted butter"/>
  <hasIngredient rdf:resource="#confectioners' sugar"/>
