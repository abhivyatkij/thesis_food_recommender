<owl:NamedIndividual rdf:about="#Bitter Gourd Stir Fry or Kakarakaya Fry | How to make Bitter Gourd Fry Without Bitterness | Easy Karela Fry Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#bitter gourds"/>
  <hasIngredient rdf:resource="#salt"/>
