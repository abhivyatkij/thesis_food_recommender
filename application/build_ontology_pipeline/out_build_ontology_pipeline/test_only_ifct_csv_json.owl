  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">40 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">World</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Appetizer / Snack, brunch</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
