<owl:NamedIndividual rdf:about="#Instant Jalebi Recipe Without Yeast | Crispy Homemade Jalebi Recipe  How to make Jalebi Crispy">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#all-purpose flour"/>
  <hasIngredient rdf:resource="#rice flour"/>
  <hasIngredient rdf:resource="#salt"/>
