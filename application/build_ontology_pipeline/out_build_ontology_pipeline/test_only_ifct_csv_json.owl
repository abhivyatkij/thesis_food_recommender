  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Appetizer/Maincourse</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
