<owl:NamedIndividual rdf:about="#Mango Pomegranate Guacamole  Best Ever Guacamole Recipe | Mexican Dip made with Avocados | Easy Avocado Dip Vegan">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#garlic"/>
