<owl:NamedIndividual rdf:about="#Masala Akki Roti Recipe | Rice Flour Roti Recipe | Akki Rotti Recipe  Karnataka Special Breakfast | How to make Akki Roti Soft">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#rice flour"/>
  <hasIngredient rdf:resource="#salt"/>
  <hasIngredient rdf:resource="#dill leaves"/>
  <hasIngredient rdf:resource="#carrot"/>
  <hasIngredient rdf:resource="#onion"/>
  <hasIngredient rdf:resource="#coriander"/>
  <hasIngredient rdf:resource="#curry leaves"/>
  <hasIngredient rdf:resource="#green chillies"/>
  <hasIngredient rdf:resource="#ginger"/>
  <hasIngredient rdf:resource="#fresh coconut"/>
  <hasIngredient rdf:resource="#cumin seeds"/>
