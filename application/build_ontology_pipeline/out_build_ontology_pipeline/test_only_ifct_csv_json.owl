<owl:NamedIndividual rdf:about="#Menthi Kura Pappu Recipe  Methi Dal Fry | Fenugreek Dal |  Methi Dal Recipe | Methi Pappu | Dal Methi Recipe Video">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasIngredient rdf:resource="#oil"/>
  <hasIngredient rdf:resource="#dry red chillies"/>
  <hasIngredient rdf:resource="#mustard seeds"/>
