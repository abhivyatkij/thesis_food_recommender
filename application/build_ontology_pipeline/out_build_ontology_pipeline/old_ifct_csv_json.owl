<owl:NamedIndividual rdf:about="#Crispy Adai Dosa Recipe  Mixed Lentil Dosa | South Indian Adai Dosa Recipe (Protein Rich Dosa Recipe)">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P03-0004744</hasRecipeID>
