  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indian (South Indian)</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dessert / Festival Sweet, Sweet / Festival Recipe</hasCourse>
