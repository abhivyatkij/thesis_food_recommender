<owl:NamedIndividual rdf:about="#Vazhakkai Varuval Recipe | Raw Banana South Indian Recipe | Vazhakkai Fry (Roast) | Plantain Fry Indian Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
