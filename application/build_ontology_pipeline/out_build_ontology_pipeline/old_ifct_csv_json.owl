<owl:NamedIndividual rdf:about="#Tandoori chicken with Mint Coriander relish">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0005132</hasRecipeID>
