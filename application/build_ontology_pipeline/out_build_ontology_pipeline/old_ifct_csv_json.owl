<owl:NamedIndividual rdf:about="#Kohlrabi Kurma  Kohlrabi Recipe Indian | Noolkol Kurma | Ganth Gobhi Ki Sabzi Recipe | Turnip Kurma">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0005114</hasRecipeID>
