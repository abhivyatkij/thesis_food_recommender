  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sugar</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">yoghurt</hasActualIngredients>
