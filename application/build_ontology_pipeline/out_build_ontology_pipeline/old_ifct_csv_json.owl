  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">45 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indian (Andhra)</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dessert/Snack, Festival Recipe, Prasadam / Festive offering</hasCourse>
