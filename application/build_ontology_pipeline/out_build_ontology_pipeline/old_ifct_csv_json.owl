<owl:NamedIndividual rdf:about="#Potato Bread Rolls Recipe Indian  Bread Bonda Recipe | Aloo Bread Bonda | Easy Bread Potato Rolls">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P05-0004775</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.masalakorb.com/potato-bread-rolls-recipe-aloo-bread-bonda/</hasRecipeURL>
  <hasPrepTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">15 minutes</hasPrepTime>
  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
