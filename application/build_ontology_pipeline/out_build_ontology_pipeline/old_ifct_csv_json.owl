<owl:NamedIndividual rdf:about="#Punugulu With Dosa Batter  How to make Punugulu | Idli Pindi Punugulu  Punugulu Recipe With Leftover Idli Dosa Batter">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P06-0004792</hasRecipeID>
