  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">butter</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">salt</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sago</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">channa dal</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">green chillies</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ginger</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">curry leaves</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cumin seeds</hasActualIngredients>
