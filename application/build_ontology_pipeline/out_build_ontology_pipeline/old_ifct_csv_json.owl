<owl:NamedIndividual rdf:about="#Sweet Boondi Recipe Step by Step | Meethi Boondi Recipe | How to make Boondi  Bundi Sweet">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0004859</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.masalakorb.com/sweet-boondi-recipe-meethi-boondi/</hasRecipeURL>
