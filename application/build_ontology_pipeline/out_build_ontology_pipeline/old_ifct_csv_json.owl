<owl:NamedIndividual rdf:about="#Hotel Tiffin Sambar Recipe | Recipe For Tiffin Sambar | Hotel Style Sambar Recipe | Hotel Idli Sambar | Quick Sambar For Idli, Dosa, Pongal, Vada">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P03-0004739</hasRecipeID>
