<owl:NamedIndividual rdf:about="#Strawberry Coconut Burfi With Khoya | Strawberry Coconut Fudge | Holi Recipes">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0005068</hasRecipeID>
