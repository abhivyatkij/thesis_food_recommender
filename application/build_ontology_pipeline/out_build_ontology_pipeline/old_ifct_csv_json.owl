<owl:NamedIndividual rdf:about="#Easy Chocolate Dipped Strawberries Recipe  | Chocolate Covered Strawberries Recipe Microwave | How to make Chocolate Dipped Strawberries at home | Valentines Day Celebration Ideas">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P05-0004776</hasRecipeID>
