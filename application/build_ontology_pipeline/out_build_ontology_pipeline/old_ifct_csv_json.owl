<owl:NamedIndividual rdf:about="#Tandoori Paneer Tikka With Grilled Veggies">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0005133</hasRecipeID>
