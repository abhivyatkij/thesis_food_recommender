<owl:NamedIndividual rdf:about="#Kerala Unniyappam Recipe | Nei Appam Recipe | Banana Paniyaram Recipe | Sweet Appam | Instant Sweet Paniyaram Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0004919</hasRecipeID>
