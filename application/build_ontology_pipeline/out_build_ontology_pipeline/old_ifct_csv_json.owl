<owl:NamedIndividual rdf:about="#Chicken Dum Biryani Hyderabadi Style | Easy Chicken Dum Biryani Recipe | Murgh Dum Biryani | Low Fat Chicken Biryani">
  <rdf:type rdf:resource="#FoodRecipes"/>
