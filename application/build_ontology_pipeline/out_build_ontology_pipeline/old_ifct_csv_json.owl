<owl:NamedIndividual rdf:about="#Chicken Rasam or Indian Style Chicken Soup  Kozhi Rasam | Chicken Kulambu | Indian Chicken Soup Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
