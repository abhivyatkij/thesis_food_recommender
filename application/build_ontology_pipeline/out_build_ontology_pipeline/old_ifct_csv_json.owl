<owl:NamedIndividual rdf:about="#Vegetable Masala Khichdi | Moong Dal Khichdi Recipe | How to make Khichdi in Pressure Cooker">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0004844</hasRecipeID>
