<owl:NamedIndividual rdf:about="#Snake Gourd CurryRecipe | Potlakaya Palu Kura | Snake Gourd Recipe Andhra Style | Potlakaya Curry  Snake Gourd Curry">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0004912</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.masalakorb.com/snake-gourd-curry-recipe-potlakaya-palu-kura/</hasRecipeURL>
  <hasPrepTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10 minutes</hasPrepTime>
  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
  <hasCookTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">20 minutes</hasCookTime>
  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">30 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indian (South Indian)</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Side Dish</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
