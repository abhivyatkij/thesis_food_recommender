<owl:NamedIndividual rdf:about="#Undrallu Kudumulu Recipe | How to make Modakam | Rava Modak Recipe | Aviri Kudumulu | Rava Kudumulu | Uppu Kozhukkottai | Vinayaka Chavithi Prasadam">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0004953</hasRecipeID>
