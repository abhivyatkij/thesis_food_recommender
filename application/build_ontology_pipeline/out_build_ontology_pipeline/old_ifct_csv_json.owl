<owl:NamedIndividual rdf:about="#Uppu Seedai Kara Seedai Recipe | Uppu Cheedai Salt Seedai | Uppu Seedai Recipe | How to make Cheedai | Gokulashtami Recipes | Janmashtami Special Dishes">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P06-0004799</hasRecipeID>
