ry Red Chilies': {'category': 'HerbsAndSpices', 'form': 'NA', 'quantity': 2.0, 'unit': 'count', 'estimated_weight_in_grams': 2.0, 'notes': 'NA'}, 'Asafoetida': {'category': 'HerbsAndSpices', 'form': 'NA', 'quantity': 1.0, 'unit': 'pinch', 'estimated_weight_in_grams': 1.0, 'notes': 'NA'}, 'Curry Leaves': {'category': 'HerbsAndSpices', 'form': 'NA', 'quantity': 'NA', 'unit': 'NA', 'estimated_weight_in_grams': 'NA', 'notes': 'NA'}}}]</hasIngredientDescription>
