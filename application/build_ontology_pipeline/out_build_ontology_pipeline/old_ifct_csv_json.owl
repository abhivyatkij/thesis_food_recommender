  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Instant Mix For Rice</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
