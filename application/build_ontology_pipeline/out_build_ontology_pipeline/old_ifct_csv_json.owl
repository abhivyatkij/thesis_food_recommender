wder': {'category': 'PlantOrigin', 'form': 'powder', 'quantity': 1.0, 'unit': 'tablespoon', 'estimated_weight_in_grams': 12.0, 'notes': 'NA'}, 'Chaat Masala': {'category': 'PlantOrigin', 'form': 'NA', 'quantity': 0.5, 'unit': 'teaspoon', 'estimated_weight_in_grams': 2.5, 'notes': 'NA'}, 'Black salt': {'category': 'PlantOrigin', 'form': 'NA', 'quantity': 0.5, 'unit': 'teaspoon', 'estimated_weight_in_grams': 2.5, 'notes': 'NA'}, 'Nutmeg piece Grated or powdered': {'category': 'PlantOrigin', 'form': 'grated, powdered', 'quantity': 1.0, 'unit': 'count', 'estimated_weight_in_grams': 5.0, 'notes': 'NA'}}}]</hasIngredientDescription>
