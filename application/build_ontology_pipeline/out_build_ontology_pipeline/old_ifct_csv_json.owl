<owl:NamedIndividual rdf:about="#Easy Homemade Strawberry Jam Recipe | Old Fashioned Strawberry Jam Recipe | How to make Strawberry Jam Without pectin | Classic Strawberry Jam">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P09-0004966</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.masalakorb.com/easy-homemade-strawberry-jam-recipe-without-pectin/</hasRecipeURL>
  <hasPrepTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">15 minutes</hasPrepTime>
  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
  <hasCookTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">30 minutes</hasCookTime>
  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">45 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">World</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jams and Preserves</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
