  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">raw rice</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cashews</hasActualIngredients>
