  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mustard seeds</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fennel seeds</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fenugreek seeds</hasActualIngredients>
  <hasActualIngredients rdf:datatype="http://www.w3.org/2001/XMLSchema#string">asafoetida</hasActualIngredients>
