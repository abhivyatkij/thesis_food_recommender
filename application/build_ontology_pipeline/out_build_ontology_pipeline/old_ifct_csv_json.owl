<owl:NamedIndividual rdf:about="#Tricolour Idli Recipe with Tricolour Chutneys  Vegetable Idli | Mini Idli Recipe (Button Idli) | Tiranga Idli  Republic Day Special">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P05-0004778</hasRecipeID>
