  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">45 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indian</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Main</hasCourse>
