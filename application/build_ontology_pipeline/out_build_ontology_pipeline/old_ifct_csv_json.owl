  <hasFermentTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasFermentTime>
  <hasCookTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">15 minutes</hasCookTime>
  <hasTotalTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">35 minutes</hasTotalTime>
  <hasCuisine rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indian, Indian (Gujarati)</hasCuisine>
  <hasCourse rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Breakfast/Main Course/Flat breads, Indian Flat breads</hasCourse>
  <hasDifficulty rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NA</hasDifficulty>
