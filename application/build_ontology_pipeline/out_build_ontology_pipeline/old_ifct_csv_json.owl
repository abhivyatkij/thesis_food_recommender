}, 'Cloves': {'category': 'PlantOrigin', 'form': 'NA', 'quantity': 4.0, 'unit': 'count', 'estimated_weight_in_grams': 2.0, 'notes': 'NA'}, 'Cardamom': {'category': 'PlantOrigin', 'form': 'NA', 'quantity': 1.0, 'unit': 'count', 'estimated_weight_in_grams': 3.0, 'notes': 'NA'}, 'Cashews': {'category': 'PlantOrigin', 'form': 'NA', 'quantity': 10.0, 'unit': 'count', 'estimated_weight_in_grams': 50.0, 'notes': 'NA'}}}, {'heading': 'Other Ingredients For Gravy:', 'items': {'oil': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 1.0, 'unit': 'tablespoon', 'estimated_weight_in_grams': 14.79, 'notes': 'NA'}, 'butter': {'category': 'animalOrigin', 'form': 'NA', 'quantity': 1.0, 'unit': 'count', 'estimated_weight_in_grams': 113.0, 'notes': 'NA'}, 'bay leaf': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 1.0, 'unit': 'count', 'estimated_weight_in_grams': 0.8, 'notes': 'NA'}, 'cinnamon stick': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 0.5, 'unit': 'count', 'estimated_weight_in_grams': 1.0, 'notes': 'NA'}, 'cloves': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 2.0, 'unit': 'count', 'estimated_weight_in_grams': 1.0, 'notes': 'NA'}, 'cardamom': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 1.0, 'unit': 'count', 'estimated_weight_in_grams': 0.13, 'notes': 'NA'}, 'red chilli powder': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 1.0, 'unit': 'teaspoon', 'estimated_weight_in_grams': 2.0, 'notes': 'NA'}, 'turmeric powder': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 0.5, 'unit': 'teaspoon', 'estimated_weight_in_grams': 1.5, 'notes': 'NA'}, 'salt': {'category': 'chemicalFoodProduct', 'form': 'NA', 'quantity': 'NA', 'unit': 'NA', 'estimated_weight_in_grams': 'NA', 'notes': 'to taste'}, 'water': {'category': 'chemicalFoodProduct', 'form': 'NA', 'quantity': 0.5, 'unit': 'cup', 'estimated_weight_in_grams': 118.0, 'notes': 'NA'}, 'sugar': {'category': 'plantOrigin', 'form': 'NA', 'quantity': 1.0, 'unit': 'teaspoon', 'estimated_weigh