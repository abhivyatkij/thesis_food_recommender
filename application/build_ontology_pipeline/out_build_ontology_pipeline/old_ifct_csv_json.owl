<owl:NamedIndividual rdf:about="#Palak Paneer Curry  Spinach Paneer Recipe | Healthy Palak Paneer Without Cream | Easy Saag Paneer Recipe">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P07-0004812</hasRecipeID>
