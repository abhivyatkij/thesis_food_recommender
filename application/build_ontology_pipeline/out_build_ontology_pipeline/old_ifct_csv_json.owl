<owl:NamedIndividual rdf:about="#PALA MUNJALU RECIPE ANDHRA STYLE  SEMOLINA DUMPLING WITH LENTIL FILLING | HOW TO MAKE PAALA MUNJALU SWEET | PALA MUNJALU  TRADITIONAL INDIAN SWEET">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P02-0004722</hasRecipeID>
  <hasRecipeURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.masalakorb.com/pala-munjalu-recipe-andhra-sweet/</hasRecipeURL>
  <hasPrepTime rdf:datatype="http://www.w3.org/2001/XMLSchema#string">25 minutes</hasPrepTime>
