<owl:NamedIndividual rdf:about="#Usirikaya Pachadi Recipe | Amla Pickle Andhra Style | Indian Gooseberry Pickle | Usiri Avakaya (Usiri Uragaya)">
  <rdf:type rdf:resource="#FoodRecipes"/>
  <hasRecipeID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HFO:IND-['MK']-P03-0004747</hasRecipeID>
